{
  "manifest_version": 3,
  "name": "Auto Tab Closer",
  "version": "1.3",
  "description": "Ferme automatiquement les onglets contenant 'Login Success' dans l'URL.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  }
}
